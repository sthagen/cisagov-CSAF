  "vulnerabilities": [
    {
      "cve": "CVE-2025-20061",
      "cwe": {
        "id": "CWE-78",
        "name": "Improper Neutralization of Special Elements used in an OS Command ('OS Command Injection')"
      },
      "notes": [
        {
          "category": "summary",
          "text": "mySCADA myPRO does not properly neutralize POST requests sent to a specific port with email information. This vulnerability could be exploited by an attacker to execute arbitrary commands on the affected system.",
          "title": "Vulnerability Summary"
        }
      ],
